<view class="sunSetBox">
	<view class="sunSetBg">
		<view class="sunShadowBox">
			<view class="{{$.themeValue == 'light'?'lightSunShadow':'darkSunShadow'}}">
				<!-- <view class="sunMove" style="margin-left: {{sunIconBgLef}}px;"></view> -->
			</view>
			<view class="{{$.themeValue == 'light'?'lightSunRiseShadow':'darkSunRiseShadow'}}" style="transform: translateY(100%) rotate({{rotateAangle}}deg);transform-origin: 50% 0%;">
			</view>
		</view>
		<image src="{{$.themeValue == 'light'?'https://weather.ioslide.com/weather/sunRistSet-light.svg':'https://weather.ioslide.com/weather/sunRistSet-dark.svg'}}" class="sunIcon" style="margin-left: {{sunIconLef}}px; margin-bottom: {{sunIconBottm}}px;"></image>
	</view>
	<!-- <view class="sunSetTime">
		<view class="sunRise">{{sunrise}}</view>
		<view class="sunSet">{{sunset}}</view>
	</view> -->
</view>