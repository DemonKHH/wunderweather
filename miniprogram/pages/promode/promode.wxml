<view class="cu-custom">
  <view class="cu-bar fixed {{bgImage!=''?'none-bg text-white bg-img':''}} {{bgColor}}" style="height:{{CustomBar}}px;padding-top:{{StatusBar}}px;{{bgImage?'background-image:url(' + bgImage+')':''}}">
    <view class="action border-custom" style="width:{{Custom.width}}px;height:{{Custom.height}}px;margin-left:calc(750rpx - {{Custom.right}}px)">
      <text class="cuIcon-back" bindtap="BackPage"></text>
      <button open-type="share" style="background:none;border:none">
        <text class="cuIcon-upload"></text>
      </button>

    </view>
    <view class="content" style="top:{{StatusBar}}px">
      <view class="{{headContentSwitch==false?'fadeOutUp dnone':'fadeInUp'}}">{{headContentcurTime}}</view>
      <view class="{{headContentSwitch==false?'fadeInUp':'fadeOutUp dnone'}}">{{forecastData.city}} </view>
      <slot name="content"></slot>
    </view>
    <slot name="right"></slot>
  </view>
</view>



<view class="padding" style="padding-top: {{CustomBar}}px;">
  <view class="body">
    <view icon="sunny" class="icon sunny" data-label="Sunny">
      <span class="sun"></span>
    </view>

    <view icon="cloudy" class="icon cloudy" data-label="Perfect">
      <span class="cloud"></span>
      <span class="cloud"></span>
    </view>

    <view icon="snowy" class="icon snowy " data-label="Chilly">
      <span class="snowman"></span>
      <ul>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
      </ul>
    </view>

    <view icon="stormy" class="icon stormy" data-label="Soggy">
      <span class="cloud"></span>
      <ul>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
        <li></li>
      </ul>
    </view>

    <view icon="supermoon" class="icon supermoon" data-label="Cool!">
      <span class="moon"></span>
      <span class="meteor"></span>
    </view>

  </view>

  <form>
    <view class="cu-form-group">
      <text class="text-grey">晨日进入地平线时间</text>
      <switch class="switch-blue" disabled="true" checked="{{switch2Checked}}"></switch>
    </view>

    <view class="cu-form-group">
      <text class="text-grey">晨日离开地平线时间</text>
      <switch class="switch-blue" disabled="true" checked="{{switch2Checked}}"></switch>
    </view>

    <view class="cu-form-group">
      <text class="text-grey">早晨最佳摄影黄金时间</text>
      <switch class="switch-blue" disabled="true" checked="{{switch2Checked}}"></switch>
    </view>
    <view class="cu-form-group">
      <text class="text-grey">太阳正午时间</text>
      <switch class="switch-blue" disabled="true" checked="{{switch2Checked}}"></switch>

    </view>
    <view class="cu-form-group">
      <text class="text-grey">落日底部边缘触及地平线时间</text>
      <switch class="switch-blue" disabled="true" checked="{{switch2Checked}}"></switch>

    </view>
    <view class="cu-form-group">
      <text class="text-grey">傍晚黄昏开始时间</text>
      <switch class="switch-blue" disabled="true" checked="{{switch2Checked}}"></switch>

    </view>
    <view class="cu-form-group">
      <text class="text-grey">夜晚开始时间</text>
      <switch class="switch-blue" disabled="true" checked="{{switch2Checked}}"></switch>

    </view>
    <view class="cu-form-group">
      <text class="text-grey">夜晚结束时间</text>
      <switch class="switch-blue" disabled="true" checked="{{switch2Checked}}"></switch>

    </view>
    <view class="cu-form-group">
      <text class="text-grey">早晨航海曙光开始时间</text>
      <switch class="switch-blue" disabled="true" checked="{{switch2Checked}}"></switch>

    </view>
    <view class="cu-form-group">
      <text class="text-grey">晨间航海黄昏结束时间</text>
      <switch class="switch-blue" disabled="true" checked="{{switch2Checked}}"></switch>

    </view>
    <view class="cu-form-group">
      <text class="text-grey">月出时间</text>
      <switch class="switch-blue" disabled="true" checked="{{switch2Checked}}"></switch>

    </view>
    <view class="cu-form-group">
      <text class="text-grey">月落时间</text>
      <switch class="switch-blue" disabled="true" checked="{{switch2Checked}}"></switch>

    </view>
    <view class="cu-form-group">
      <text class="text-grey">月球距离</text>
      <switch class="switch-blue" disabled="true" checked="{{switch2Checked}}"></switch>

    </view>
    <view class="cu-form-group">
      <text class="text-grey">月球方位角</text>
      <switch class="switch-blue" disabled="true" checked="{{switch2Checked}}"></switch>
    </view>
    <view class="cu-form-group">
      <text class="text-grey">月亮高于地平线的高度</text>
      <switch class="switch-blue" disabled="true" checked="{{switch2Checked}}"></switch>
    </view>
    <view class="cu-form-group">
      <text class="text-grey">月球平行角</text>
      <switch class="switch-blue" disabled="true" checked="{{switch2Checked}}"></switch>
    </view>
  </form>
</view>